        function CnfSubmenumsg(msg, Url) {
            debugger;
            $("#Id").val();
            $.ajax(
                {
                    url: Url,
                    beforeSend: function () {

                    },
                    type: "Get",
                    success: function (data) {
                        $(".se-pre-con").hide();
